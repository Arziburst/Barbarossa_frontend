    },
    "scripts": {
        "start": "cross-env-shell NODE_ENV=development ts-node -P \"./tsconfig.webpack.json\" \"./scripts/webpack/start\"",
        "build": "cross-env-shell NODE_ENV=production ts-node -P \"./tsconfig.webpack.json\" \"./scripts/webpack/build\"",
        "gen": "cross-env-shell ts-node -P \"./tsconfig.generate.json\" \"./scripts/generate\"",
        "analyze": "webpack-bundle-analyzer \"./build/stats.json\"",
        "serve": "serve \"./build\" -s",
        "graphgen": "npm run apollo:push && npm run apollo:schema && npm run apollo:gen",
        "apollo:push": "apollo service:push --endpoint https://graphql.barbarossa.pp.ua/graphql",
        "apollo:schema": "apollo service:download --endpoint https://graphql.barbarossa.pp.ua/graphql graphqlSchema.json",
        "apollo:gen": "apollo codegen:generate --localSchemaFile=graphqlSchema.json --target=typescript --globalTypesFile=src/init/@types/graphql-global-types.ts \"../types\""
    },
